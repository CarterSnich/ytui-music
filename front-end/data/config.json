{
    // Duration in ms to refresh the ui 
    "refresh_rate": 900, // Default 900
    // Single char key to navigate back and forth result in musicbar/playlistbar/artistbar
    "page_nav_next": 'n', // Default 'n'
    "page_nav_prev": 'p', // Default 'p'
    // Single char key to move to searchbar
    "search_key": '/', // Default '/'
    // Single char key to quit the application.
    "quit_key": 'q', // Default 'q'
    // Single char key to (un)pause the playback
    "toggle_play": ' ', // Default ' '
    // Single char key to show help
    "show_help": '?', // Default '?'
    // Single char key to seek forward/backward the playback
    "seek_forward": '>', // Default '>'
    "seek_backward": '<', // Default '<'
}
